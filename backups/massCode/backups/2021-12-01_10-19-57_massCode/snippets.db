.\r\n  \r\nln путь_к_файлу необязательное_имя_файла     # Создаёт программные ссылки на файлы. Сокращение от Link."}],"tags":[],"isFavorites":false,"isDeleted":false,"createdAt":{"$$date":1638040447663},"updatedAt":{"$$date":1638279988920},"folder":{"id":"MsSQGFOv5","name":"Terminal commands","open":false,"defaultLanguage":"text"},"tagsPopulated":[],"_id":"Wxm6Cdn2jZdwjGSz"}
